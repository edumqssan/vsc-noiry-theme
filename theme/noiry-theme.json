	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5b6171"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"source.java storage.modifier"
			],
			"settings": {
				"foreground": "#eeeff2"
			}
		},
		{
			"name": "Shell Variables",
			"scope": [
